{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "default-case": "off",
    "generator-star-spacing": "off",
    "import/no-extraneous-dependencies": "off",
    "max-len": ["error", 120],
    "new-parens": "off",
    "no-console": "off",
    "no-mixed-operators": "off",
    "no-param-reassign": ["error", {"props": false}],
    "prefer-template": "off",
    "require-yield": "off"
  }
}
